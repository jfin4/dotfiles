text/plain; $EDITOR %s ;
# firefox is first, opens attachment
text/html; DBUS_SESSION_BUS_ADDRESS=unix:path=/run/user/$(id -u)/bus firefox %s &;
text/html; lynx -assume_charset=%{charset} -display_charset=utf-8 -dump -width=1024 %s; nametemplate=%s.html; copiousoutput;
image/*; /usr/bin/feh %s ;
video/*; setsid mpv --quiet %s &; copiousoutput
audio/*; mpv %s ;
application/pdf; launch-file %s ;
application/pgp-encrypted; gpg -d '%s'; copiousoutput;
application/pgp-keys; gpg --import '%s'; copiousoutput;

application/vnd.openxmlformats-officedocument.spreadsheetml.sheet; xdg-open %s &;

text/html; cp %s %s.html\; firefox %s.html 2> /dev/null
text/html; lynx -assume_charset=%{charset} -display_charset=utf-8 -collapse_br_tags -dump %s; nametemplate=%s.html; copiousoutput
image/*; feh %s
application/pgp-keys; pgp -f < %s ; copiousoutput
application/pdf; cp %s %s.pdf\; mupdf %s.pdf &
application/vnd.openxmlformats-officedocument.wordprocessingml.document; cp %s %s.docx\; libreoffice %s.docx &

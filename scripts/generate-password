# password management
pw_gen () {
LENGTH="${1-20}"
SYMBOLS="${2-!@#\$%^&*()}"
PASSWORD_STORE_CHARACTER_SET="a-zA-Z0-9$SYMBOLS"\
    pass generate temp $LENGTH > /dev/null
unset LENGTH SYMBOLS PASSWORD_STORE_CHARACTER_SET
password=$(pass temp)
pass rm --force temp > /dev/null
echo -n $password > /dev/clipboard
} 
